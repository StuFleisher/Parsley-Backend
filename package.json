{
  "type": "module",
  "dependencies": {
    "axios": "^1.6.1",
    "dotenv": "^16.3.1",
    "express": "^5.0.0-beta.1",
    "openai": "^4.17.4",
    "pg": "^8.11.3",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*.ts"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts",
      "node_modules"
    ],
    "exec": "npm run build && node dist/server.js"
  }
}
